      "type": "custom", // SoftwareStart
      "format": "┌─────── Software: ─────────────────────────────────────────────────────────┐",
      "outputColor": "34"
    },

    // "break",
    {
      "type": "custom", // InformationEnd
      "format": "│",
      "outputColor": "34"
    },

    // "os",
    {
      "type": "os",
      "key": "│              OS",
      "keyColor": "34"
    },

    // "kernel",
    {
      "type": "kernel",
      "key": "│          Kernel",
      "keyColor": "34"
    },

    // "packages",
    {
      "type": "packages",
      "key": "│        Packages",
      "keyColor": "34"
    },

    // "terminal",
    {
      "type": "terminal",
      "key": "│        Terminal",
      "keyColor": "34"
    },

    // "shell",
    {
      "type": "shell",
      "key": "│           Shell",
      "keyColor": "34"
    },

    // "de",
    {
      "type": "de",
      "key": "│              DE",
      "keyColor": "34"
    },

    // "wm",
    {
      "type": "wm",
      "key": "│              WM",
      "keyColor": "34"
    },

    // "break",
    // {
    //   "type": "custom", // InformationEnd
    //   "format": "│",
    //   "outputColor": "34"
    // },

    // "theme",
    {
      "type": "theme",
      "key": "│           Theme",
      "keyColor": "34"
    },

    // "icons",
    {
      "type": "icons",
      "key": "│           Icons",
      "keyColor": "34"
    },

    // "font",
    {
      "type": "font",
      "key": "│            Font",
      "keyColor": "34"
    },

    // "terminalfont",
    {
      "type": "terminalfont",
      "key": "│   Terminal Font",
      "keyColor": "34"
    },

    // "break",
    {
      "type": "custom", // InformationEnd
      "format": "│",
      "outputColor": "34"
    },

    {
      "type": "custom", // InformationEnd
      "format": "└───────────────────────────────────────────────────────────────────────────┘",
      "outputColor": "34"
    },

    "break",
    {
      "type": "custom", // HardwareStart
      "format": "┌─────── Hardware: ─────────────────────────────────────────────────────────┐",
      "outputColor": "35"
    },

    // "break",
    {
      "type": "custom", // InformationEnd
      "format": "│",
      "outputColor": "35"
    },

    // "host",
    {
      "type": "host",
      "key": "│            Host",
      "keyColor": "35"
    },

