      "outputColor": "35"
    },

    {
      "type": "custom", // InformationEnd
      "format": "└───────────────────────────────────────────────────────────────────────────┘",
      "outputColor": "35"
    },

    "break",
    {
      "type": "custom", // NetworkStart
      "format": "┌──────── Network: ─────────────────────────────────────────────────────────┐",
      "outputColor": "36"
    },

    // "break",
    {
      "type": "custom", // InformationEnd
      "format": "│",
      "outputColor": "36"
    },

    // "localip",
    {
      "type": "localip",
      "key": "│        Local IP",
      "keyColor": "36"
    },

    // "publicip",
    {
      "type": "publicip",
      "key": "│       Public IP",
      "keyColor": "36"
    },

    // "break",
    {
      "type": "custom", // InformationEnd
      "format": "│",
      "outputColor": "36"
    },

    {
      "type": "custom", // InformationEnd
      "format": "└───────────────────────────────────────────────────────────────────────────┘",
      "outputColor": "36"
    },

    "break",
    {
      "type": "custom", // UptimeStart
      "format": "┌── Miscellaneous: ─────────────────────────────────────────────────────────┐",
      "outputColor": "37"
    },

    // "break",
    {
      "type": "custom", // InformationEnd
      "format": "│",
      "outputColor": "37"
    },

    // "uptime",
    {
      "type": "uptime",
      "key": "│          Uptime",
      "keyColor": "37"
    },

    {
      "type": "command",
      "key": "│             Age",
      "text": "birth_install=$(stat -c %W /); current=$(date +%s); days_difference=$(( (current - birth_install) / 86400 )); echo $days_difference days",
      "keyColor": "37"
    },

    // "locale",
    {
      "type": "locale",
      "key": "│          Locale",
      "keyColor": "37"
    },

    // "datetime",
    {
      "type": "datetime",
      "key": "│            Date",
      "keyColor": "37"
    },

    // "break",
    {
      "type": "custom", // InformationEnd
      "format": "│",
      "outputColor": "37"
    },

    {
      "type": "custom", // InformationEnd
      "format": "└───────────────────────────────────────────────────────────────────────────┘",
      "outputColor": "37"
    },

    "break",

    // "colors",
    {
      "type": "colors",
      "paddingLeft": 30,
      "symbol": "circle"
    },
    "break",
    "break"
  ]
