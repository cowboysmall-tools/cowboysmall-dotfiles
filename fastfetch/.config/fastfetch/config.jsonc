    {
      "type": "cpu",
      "key": "│             CPU",
      "keyColor": "35"
    },

    // "gpu",
    {
      "type": "gpu",
      "key": "│             GPU",
      "keyColor": "35"
    },

    // "display",
    {
      "type": "display",
      "key": "│         Display",
      "keyColor": "35"
    },

    // "memory",
    {
      "type": "memory",
      "key": "│          Memory",
      "keyColor": "35"
    },

    // "disk",
    {
      "type": "disk",
      "key": "│            Disk",
      "keyColor": "35"
    },

    // "break",
    {
      "type": "custom", // InformationEnd
